{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"cache": false
		},
		"clean": {
			"cache": false,
			"dependsOn": ["^clean"]
		},
		"preview": {
			"cache": false,
			"dependsOn": ["build"],
			"persistent": true
		},
		"lint": {},
		"typecheck": {}
	}
}
